import numpy as np
import pandas as pd

np.random.seed(1)
# Create A and b matrice with pandas
A = 10* np.random.rand(10,10) - 5
b = 10* np.random.rand(10) - 5

# solve Ax = b

solution = np.linalg.solve(A,b)
solution

# to verifiy the solution Ax - b = 0
sum(abs(np.dot(A, solution) - b))
print('Solution: ', solution)
